version: 1

formatters:
  precise:
    format: '%(asctime)s - %(name)s - %(lineno)d - %(levelname)s - %(request)s - %(message)s'

handlers:
  console:
    class: logging.StreamHandler
    formatter: precise

loggers:
    _placeholder:
        level: "WARN"
    synapse.storage.SQL:
        level: WARN

root:
    level: WARN
    handlers: [console]

disable_existing_loggers: false
